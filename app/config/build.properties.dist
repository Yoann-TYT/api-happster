# Database driver
propel.database = mysql
propel.database.url = mysql:host=localhost;dbname=happster
propel.database.user = root
propel.database.password =

# Project name
propel.project = happster

propel.output.dir = ${propel.project.dir}/../lib/Happster/Model
propel.php.dir = ${propel.output.dir}
propel.targetPackage =

propel.phpconf.dir = ${propel.project.dir}

propel.sql.dir = ${propel.project.dir}/../data/sql

propel.runtime.phpconf.file = ${propel.project}-conf.php
propel.runtime.phpconf-classmap.file = ${propel.project}-classmap.php

# Add your custom behavior paths here
propel.behavior.blamable.class = ${propel.project.dir}/../lib.Happster\Propel\Behavior\BlamableBehavior
